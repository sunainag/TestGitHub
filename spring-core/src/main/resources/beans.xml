<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/util 
    http://www.springframework.org/schema/util/spring-util.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">

	<context:annotation-config />

	<bean id="student" class="com.sapient.model.Student">
		<property name="age" value="10" />
		<property name="name" value="Sunaina" />
	</bean>

	<bean id="address1" class="com.sapient.model.Address">
		<property name="address" value="Gurgaon" />
		<property name="pinCode" value="122016" />
	</bean>
	
	<bean id="address2" class="com.sapient.model.Address">
		<property name="address" value="Noida" />
		<property name="pinCode" value="122011" />
	</bean>
</beans>